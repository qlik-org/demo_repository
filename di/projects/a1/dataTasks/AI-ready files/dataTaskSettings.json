{
  "fullLoadOnly": false,
  "filePublishAppSettings": {
    "sourceConnectionId": "${ai_ready_files-rwia.publishing_connection.source_connection_id}",
    "vectorDbConnectionId": "${ai_ready_files-rwia.publishing_connection.vector_db_connection_id}",
    "llmConnectionId": "${ai_ready_files-rwia.publishing_connection.llm_connection_id}",
    "index": {
      "compute": "qlik_vector_compute",
      "schema": "oriai_ready_files",
      "name": "$prefix$ai_ready_files"
    },
    "indexActionOnReload": "NONE",
    "dataAppRuntime": {
      "reportEvery": 1,
      "chunkSize": 1000,
      "chunkOverlap": 0
    },
    "k": 2,
    "filesLocation": {
      "donePath": "./done",
      "failPath": "./fail",
      "contextType": "MOVE_FILES",
      "deepScanLevel": 1
    },
    "publishTo": "VECTOR_DB"
  }
}